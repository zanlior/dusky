{
  "window.menuBarVisibility": "toggle",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.enabled": false,
  "editor.minimap.side": "right",
  "extensions.autoUpdate": false,
  "workbench.statusBar.visible": false,
  "terminal.external.linuxExec": "kitty",
  "terminal.explorerKind": "both",
  "terminal.sourceControlRepositoriesKind": "both",
  "telemetry.telemetryLevel": "off",
  "workbench.activityBar.location": "top",
  "window.customTitleBarVisibility": "auto",
  "workbench.sideBar.location": "right",
  "window.controlsStyle": "hidden",
  "workbench.colorTheme": "Material Code",
  "material-code.colors": {
    "primary": "{{colors.surface.dark.hex}}",
    "foreground": "{{colors.on_surface.dark.hex}}",
    "mutedForeground": "{{colors.primary.dark.hex}}",
    "background": "{{colors.surface.dark.hex}}",
    "card": "{{colors.surface_container.dark.hex}}",
    "popover": "{{colors.surface_container_highest.dark.hex}}",
    "hover": "{{colors.surface_container_high.dark.hex}}",
    "border": "{{colors.outline.dark.hex}}",
    "primaryForeground": "{{colors.on_surface.dark.hex}}",
    "secondary": "{{colors.secondary.dark.hex}}",
    "secondaryForeground": "{{colors.secondary_container.dark.hex}}",
    "error": "{{colors.error.dark.hex}}",
    "errorForeground": "{{colors.on_error.dark.hex}}",
    "success": "{{colors.tertiary.dark.hex}}",
    "warning": "{{colors.error_container.dark.hex}}",
    "syntax.comment": "{{colors.on_surface_variant.dark.hex}}",
    "syntax.string": "{{colors.tertiary.dark.hex}}",
    "syntax.keyword": "{{colors.secondary.dark.hex}}",
    "syntax.variable": "{{colors.on_surface.dark.hex}}",
    "syntax.attribute": "{{colors.primary.dark.hex}}",
    "syntax.property": "{{colors.on_secondary_container.dark.hex}}",
    "syntax.function": "{{colors.primary.dark.hex}}",
    "syntax.constant": "{{colors.tertiary.dark.hex}}",
    "syntax.bracket1": "{{colors.secondary_fixed.dark.hex}}",
    "syntax.bracket2": "{{colors.secondary_fixed.dark.hex}}",
    "syntax.bracket3": "{{colors.secondary_fixed.dark.hex}}",
    "syntax.bracket4": "{{colors.secondary_fixed.dark.hex}}"
  }
}
